{
  "name": "@label/cour-de-cassation",
  "version": "0.0.0",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./dist",
    "clean:all": "rimraf -rf ./dist && rimraf -rf ./node_modules",
    "compile": "tsc -p tsconfig.json",
    "fix": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "start": "RUN_MODE=LOCAL node dist/labelServer.js --settings settings/settings.json",
    "test": "RUN_MODE=TEST jest -c jest.config.json",
    "test:coverage": "RUN_MODE=TEST jest --passWithNoTests -c jest.config.json --coverage"
  },
  "dependencies": {
    "@label/backend": "*",
    "@label/core": "*",
    "@types/jest": "^26.0.13",
    "ansi-regex": "^6.0.1",
    "axios": "^1.8.2",
    "dotenv": "^16.4.5",
    "fast-xml-parser": "^3.17.4",
    "lodash": "^4.17.21",
    "string-template": "^1.0.0",
    "typescript": "~4.0.0",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/yargs": "11.0.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "24.9.0",
    "nodemon": "3.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  }
}
