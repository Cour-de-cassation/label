{
  "dependencies": {
    "@label/core": "*",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.2.2",
    "iconv-lite": "^0.6.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^5.6.0",
    "sder": "https://github.com/Cour-de-cassation/sder#56ce23301c20eb386cefccab1937158e252d0c5c",
    "sder-core": "https://github.com/Cour-de-cassation/sder-core#936abba905f8832e51138b9fb387f7cd91b5db38"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.2.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "core-js": "^3.30.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/",
    "setupFiles": [
      "core-js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setupTests.ts"
    ],
    "testRegex": ".spec.ts$",
    "testEnvironment": "node"
  },
  "main": "dist/index",
  "name": "@label/backend",
  "scripts": {
    "bootstrap": "node ./dist/scripts/bootstrap.js",
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./dist && rimraf -rf ./build",
    "cleanAll": "yarn run clean && rimraf -rf ./node_modules",
    "compile": "tsc -p tsconfig.json",
    "dockerBuild": "docker-compose build",
    "dockerStart": "docker-compose up",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "fix": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "RUN_MODE=TEST jest --passWithNoTests",
    "type": "yarn build"
  },
  "types": "dist/index",
  "version": "1.0.0"
}
