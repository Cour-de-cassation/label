{
  "dependencies": {
    "@label/core": "*",
    "@sendgrid/mail": "^7.2.6",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fast-xml-parser": "^3.17.4",
    "iconv-lite": "^0.6.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.6.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "core-js": "^3.6.5",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "24.9.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "typescript": "~4.0.0"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/",
    "setupFiles": [
      "core-js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setupTests.ts"
    ],
    "testRegex": ".spec.ts$",
    "testEnvironment": "node"
  },
  "main": "dist/index",
  "name": "@label/backend",
  "scripts": {
    "bootstrap": "node ./dist/scripts/bootstrap.js",
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./dist && rimraf -rf ./build",
    "cleanAll": "yarn run clean && rimraf -rf ./node_modules",
    "compile": "tsc -p tsconfig.json",
    "dockerBuild": "docker-compose build",
    "dockerStart": "docker-compose up",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "test": "RUN_MODE=TEST jest --passWithNoTests",
    "type": "yarn build"
  },
  "types": "dist/index",
  "version": "1.0.0"
}
