{
  "homepage": "/label",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@label/core": "*",
    "axios": "^1.4.0",
    "crypto-browserify": "^3.12.0",
    "dateformat": "^5.0.3",
    "http2": "^3.3.7",
    "pelta-design-system": "https://github.com/Cour-de-cassation/pelta-design-system#0069aa0e780ef93db34a9f6f87e1b21e210d5d50",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "^5.0.1",
    "sder-core": "https://github.com/Cour-de-cassation/sder-core#f19400e5832d88b48d076b99bbea3e4cfd6803f4",
    "stream-http": "^3.2.0",
    "string-template": "^1.0.0",
    "styled-components": "^5.3.11",
    "superstatic": "^9.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/axios": "^0.14.0",
    "@types/dateformat": "^5.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.4",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/string-template": "^1.0.2",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__dom": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "react-app-rewired": "^2.2.1",
    "react-dev-utils": "^12.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/",
    "setupFiles": [
      "core-js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setupTests.ts"
    ],
    "testRegex": ".spec.ts$",
    "testEnvironment": "node"
  },
  "name": "@label/client",
  "private": true,
  "resolutions": {
    "styled-components": "^5"
  },
  "scripts": {
    "build": "yarn clean && react-app-rewired build",
    "clean": "rimraf -rf ./dist && rimraf -rf ./build",
    "cleanAll": "yarn clean && rimraf -rf ./node_modules",
    "compile": "tsc -p tsconfig.json",
    "dockerBuild": "docker-compose build",
    "dockerStart": "docker-compose up",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "fix": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "PORT=55432 react-app-rewired start",
    "test": "RUN_MODE=TEST jest --passWithNoTests"
  },
  "version": "1.0.0"
}
