    "dockerBuild": "docker-compose build",
    "dockerStart": "docker-compose up",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "fix": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
