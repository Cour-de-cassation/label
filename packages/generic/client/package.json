{
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@label/core": "*",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.20.0",
    "http2": "^3.3.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "serve": "^11.3.2",
    "string-template": "^1.0.0",
    "styled-components": "4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.55",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/string-template": "^1.0.2",
    "@types/styled-components": "4.0.3",
    "@types/testing-library__dom": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "jest": "24.9.0",
    "prettier": "^2.1.2",
    "react-dev-utils": "^10.2.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/",
    "setupFiles": [
      "core-js"
    ],
    "setupFilesAfterEnv": [
      "./tests/setupTests.ts"
    ],
    "testRegex": ".spec.ts$",
    "testEnvironment": "node"
  },
  "name": "@label/client",
  "private": true,
  "resolutions": {
    "styled-components": "^5"
  },
  "scripts": {
    "build": "yarn clean && react-scripts build",
    "clean": "rimraf -rf ./dist && rimraf -rf ./build",
    "cleanAll": "yarn clean && rimraf -rf ./node_modules",
    "compile": "tsc -p tsconfig.json",
    "dockerBuild": "docker-compose build",
    "dockerStart": "docker-compose up",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "start": "PORT=55432 react-scripts start",
    "test": "RUN_MODE=TEST jest --passWithNoTests"
  },
  "version": "1.0.0"
}
