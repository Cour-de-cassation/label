{
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "lodash": "^4.17.21",
    "mongodb": "^5.6.0",
    "string-template": "^1.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/mongodb": "^4.0.7",
    "@types/string-template": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sder-core": "https://github.com/Cour-de-cassation/sder-core#f19400e5832d88b48d076b99bbea3e4cfd6803f4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "testEnvironment": "node"
  },
  "main": "dist/index",
  "name": "@label/core",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./dist",
    "cleanAll": "rimraf -rf ./dist && rimraf -rf ./node_modules",
    "compile": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "fix": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "node dist/app.js",
    "test": "RUN_MODE=TEST jest --passWithNoTests",
    "type": "yarn build"
  },
  "types": "dist/index",
  "version": "1.0.0"
}
