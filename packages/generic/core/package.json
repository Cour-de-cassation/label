{
  "name": "@label/core",
  "version": "1.0.0",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./dist",
    "clean:all": "rimraf -rf ./dist && rimraf -rf ./node_modules",
    "compile": "tsc -p tsconfig.json",
    "fix": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "test": "RUN_MODE=TEST jest --passWithNoTests",
    "test:coverage": "RUN_MODE=TEST jest --passWithNoTests --coverage"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/mongodb": "^3.5.27",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.1",
    "dbsder-api-types": "^1.6.2",
    "string-template": "^1.0.0",
    "typescript": "~4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/string-template": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "jest": "24.9.0",
    "prettier": "^2.1.2",
    "rimraf": "~3.0.2",
    "sder-core": "https://github.com/Cour-de-cassation/sder-core#8afb44f74c9ffefbbadc6f0d3de0def1c916dcc7"
  }
}
