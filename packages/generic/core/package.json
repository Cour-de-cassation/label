{
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.1",
    "string-template": "^1.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.19",
    "@types/bcryptjs": "^2.4.3",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.198",
    "@types/mongodb": "^3.5.27",
    "@types/string-template": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "~5.0.1",
    "sder-core": "https://github.com/Cour-de-cassation/sder-core#f19400e5832d88b48d076b99bbea3e4cfd6803f4",
    "typescript": "~5.2.2"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "testEnvironment": "node"
  },
  "main": "dist/index",
  "name": "@label/core",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./dist",
    "cleanAll": "rimraf -rf ./dist && rimraf -rf ./node_modules",
    "compile": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "fix": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "node dist/app.js",
    "test": "RUN_MODE=TEST jest --passWithNoTests",
    "type": "yarn build"
  },
  "types": "dist/index",
  "version": "1.0.0"
}
